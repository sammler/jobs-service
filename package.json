{
  "name": "jobs-service",
  "version": "0.0.13",
  "description": "Job service for sammler.",
  "keywords": [
    "sammler"
  ],
  "homepage": "https://github.com/sammler/job-service",
  "repository": "https://github.com/sammler/job-service",
  "license": "MIT",
  "author": {
    "name": "Stefan Walther",
    "url": "https://github.com/stefanwalther",
    "twitter": "waltherstefan"
  },
  "main": "src/index.js",
  "scripts": {
    "precommit": "npm run lint --quiet",
    "ensure:nodemon": "(npm list nodemon -g || npm install nodemon -g)",
    "docs": "docker run --rm -v ${PWD}:/opt/verb stefanwalther/verb",
    "lint": "eslint src && eslint test",
    "lint:fix": "eslint src --fix && eslint test --fix",
    "lint:src": "eslint src",
    "lint:src:fix": "eslint src --fix",
    "lint:test": "eslint test",
    "lint:test:fix": "eslint test --fix",
    "start:agenda-debug": "DEBUG='agenda:*' node index.js",
    "start": "node index.js",
    "start:watch": "npm run ensure:nodemon && nodemon -L --watch . --exec npm run start",
    "test": "export JWT_SECRET='My Secret' && nyc --reporter=lcov mocha './test/**/*.spec.js' --require './test/mocha.conf.js' --timeout 6000",
    "test:integration": "export JWT_SECRET='My Secret' && nyc --reporter=lcov mocha './test/integration/**/*.spec.js' --require './test/mocha.conf.js' --timeout 6000",
    "test:unit": "nyc --reporter=lcov mocha './test/unit/**/*.spec.js' --require './test/mocha.conf.js'"
  },
  "dependencies": {
    "agenda": "^2.0.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-initializers": "0.0.1",
    "helmet": "^3.15.0",
    "http-status-codes": "1.3.0",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.12",
    "mongoose-connection-config": "^0.1.2",
    "mongoose-materialized": "0.2.0",
    "mongoose-timestamp": "0.6.0",
    "read-pkg-up": "^4.0.0",
    "sammler-nodelib-logger": "0.2.4",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "1.9.7",
    "swagger-ui-express": "2.0.7",
    "winster": "^0.2.13"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-space-single": "^0.3.9",
    "eslint-plugin-mocha": "^5.2.0",
    "mocha": "5.2.0",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0"
  },
  "engines": {},
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": []
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ]
  },
  "winster": {
    "configFile": "./src/api/config/winster-config.js"
  }
}
